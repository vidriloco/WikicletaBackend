<!DOCTYPE html>
<html>
<head>
  <title><%= t('app.name') %></title>
	
	<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=true"></script>

  <%= stylesheet_link_tag  "maps", "bikes" %>
  <%= javascript_include_tag controller.controller_name %>
  <%= csrf_meta_tags %>
	<div id="fb-root"></div>
	<script>(function(d, s, id) {
	  var js, fjs = d.getElementsByTagName(s)[0];
	  if (d.getElementById(id)) return;
	  js = d.createElement(s); js.id = id;
	  js.src = "//connect.facebook.net/es_LA/all.js#xfbml=1&appId=261609860582235";
	  fjs.parentNode.insertBefore(js, fjs);
	}(document, 'script', 'facebook-jssdk'));</script>
</head>

<body>
	<div class="background-logo"></div>
	<%= render 'layouts/shared/navbar' %>
	
	<div class="row-fluid top-spaced-70">
		<div class="span1"></div>
		<div class="span10">
			<%= render 'layouts/shared/notice' %>
			<%= yield %>
		</div>
		<div class="span1"></div>
	</div>	
</body>

</html>
