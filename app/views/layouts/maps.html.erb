<!DOCTYPE html>
<html>
<head>
  <title><%= t('app.name') %> : Routes </title>
	
  <%= stylesheet_link_tag  "maps" %>
  
	<script src="http://connect.facebook.net/en_US/all.js#xfbml=1"></script>
	<script src="http://platform.twitter.com/widgets.js" type="text/javascript"></script>
	<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=true"></script>
	<%= javascript_include_tag "profiles" %>
  <%= csrf_meta_tags %>
	<%= render 'layouts/shared/fav_icons' %>
	<%= render 'layouts/shared/mix_panel' %>
</head>

<body>
	<%= render 'layouts/shared/navbar' %>
	
	<div id="content-centered">
		<div class="background-logo"></div>	
		<%= yield :panel %>
	</div>
	<div id="map-container"><div id="map" class="show-only edit <%= yield :map_behavior %>"></div></div>
</body>

</html>
